fn my_main() {
    b0 :bool := simple_eq(5, 6);
    printbln b0;

    b1:bool := big_equal(5, 5, true);
    printbln b1;
    
    b2:bool := big_equal(5, 7, true);
    printbln b2;
    
    b3:bool := lesser(3, 5);
    printbln b3;

    b4:bool := greater(3, 5);
    printbln b4;
    
    return;
}

fn simple_eq(a: i32, b: i32) -> bool {
    return a == b;
}

fn big_equal(a: i32, b: i32, c: bool) -> bool{
    return (a == b) == c;
}

fn lesser(a: i32, b: i32) -> bool {
    return a < b;
}

fn greater(a: i32, b: i32) -> bool {
    return a > b;
}