fn my_main() {
    let x:i32 := 5i32;
    let y:i32 := test();
    let z:i32 := test2(y);
    
    root::std::io::writei32ln(x);
    root::std::io::writei32ln(y);
    root::std::io::writei32ln(z);

    let w: i32 := x + y;
    root::std::io::writei32ln(w);    // 10

    let a: i32 := w + z;
    root::std::io::writei32ln(a); // 5
    
    return;
}

fn test() -> i32 {
    return 5i32;
}

fn test2(i: i32) -> i32 {
    return -i;
}