fn my_main() -> i64 {
    let c : co i64 := init my_co(MyStruct{x: 5, y: 13, z: true});
    let w : i64 := yield c;
    let z : i64 := yield c;
    root::std::io::writei64ln(w);
    root::std::io::writei64ln(z);
    return 0;
}

co my_co (x: MyStruct) -> i64 {
    root::std::io::writei64ln(x.x);
    yret x.y + x.y;
    root::std::io::writei64ln(x.y);
    return x.x * x.x;
}

struct MyStruct {
    x: i64,
    y: i64,
    z: bool,
}