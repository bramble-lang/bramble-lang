fn my_main( p: bool) {
    let c : co i32 := init my_co(MyStruct{x: 5, y: 13, z: true});
    let w : i32 := yield c;
    let z : i32 := yield c;
    printiln w;
    printiln z;
    return;
}

co my_co (x: MyStruct) -> i32 {
    printiln x.x;
    yret x.y + x.y;
    printiln x.y;
    return x.x * x.x;
}

struct MyStruct {
    x: i64,
    y: i64,
    z: bool,
}