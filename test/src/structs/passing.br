fn my_main() -> i64 {
    let t: MyStruct := MyStruct{x: 5, y: 1, z: 10};
    let mut y: i64 := test(t);
    root::std::io::writei64ln(y);

    mut y := test4(10, t);

    mut y := test2(10, t);
    root::std::io::writei64ln(y);
    
    return 0;
}

struct MyStruct {
    x: i64,
    y: i64,
    z: i64,
}

fn test(t: MyStruct) -> i64 {
    root::std::io::writei64ln(t.x);
    return t.y + t.z;
}

fn test2(x: i64, t: MyStruct) -> i64 {
    root::std::io::writei64ln(x * t.x);
    return t.y + t.z;
}

fn test3(t: MyStruct) -> i64 {
    root::std::io::writei64ln(t.x);
    root::std::io::writei64ln(t.y);
    root::std::io::writei64ln(t.z);

    return t.x + t.y + t.z;
}

fn test4(x: i64, t: MyStruct) -> i64 {
    root::std::io::writei64ln(x);
    root::std::io::writei64ln(t.x);
    root::std::io::writei64ln(t.y);
    root::std::io::writei64ln(t.z);

    return x * (t.x + t.y + t.z);
}