fn my_main() {
    let t: MyStruct := MyStruct{x: 5, y: 1, z: 10};
    let mut y: i64 := test(t);
    printiln y;

    mut y := test4(10, t);

    mut y := test2(10, t);
    printiln y;
    
    return;
}

struct MyStruct {
    x: i64,
    y: i64,
    z: i64,
}

fn test(t: MyStruct) -> i64 {
    printiln t.x;
    return t.y + t.z;
}

fn test2(x: i64, t: MyStruct) -> i64 {
    printiln x * t.x;
    return t.y + t.z;
}

fn test3(t: MyStruct) -> i64 {
    printiln t.x;
    printiln t.y;
    printiln t.z;

    return t.x + t.y + t.z;
}

fn test4(x: i64, t: MyStruct) -> i64 {
    printiln x;
    printiln t.x;
    printiln t.y;
    printiln t.z;

    return x * (t.x + t.y + t.z);
}