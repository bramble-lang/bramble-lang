
fn my_main() {
    let y:i64 := test(10, MyStruct{x: 1111, y: 2222, z: 3333});
    printiln y;
    
    return;
}

struct MyStruct {
    x: i64,
    y: i64,
    z: i32,
}

fn test(x: i64, t: MyStruct) -> i64 {
    printiln x;
    printiln t.x;
    printiln t.y;
    printiln t.z;

    return x * (t.x + t.y + t.z);
}