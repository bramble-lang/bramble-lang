
fn my_main() {
    let ms: MyStruct := MyStruct{x: 1111, y: 2222, z: 3333};
    let y:i32 := test(10, ms);
    //let y:i32 := test(10, MyStruct{x: 1111, y: 2222, z: 3333});
    printiln y;
    
    return;
}

struct MyStruct {
    x: i32,
    y: i32,
    z: i32,
}

fn test(x: i32, t: MyStruct) -> i32 {
    printiln x;
    printiln t.x;
    printiln t.y;
    printiln t.z;

    return x * (t.x + t.y + t.z);
}