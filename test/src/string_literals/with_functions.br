fn my_main() -> i64 {
    let s: string := create_a_string();
    say_something("hello, ", s);
    return 0;
}

fn create_a_string() -> string {
    return "world";
}

fn say_something(a: string, b: string) {
    root::std::io::write(a);
    root::std::io::write(b);
    root::std::io::write("\n");

    return;
}