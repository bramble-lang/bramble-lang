fn my_main() -> i64 {
    let s: string := "cadabra\n";
    let c:co string  := init my_co("abra\n", s);
    root::std::io::write((yield c));
    root::std::io::write((yield c));
    root::std::io::write((yield c));
    return 0;
}

co my_co(a: string, b: string) -> string {
    yret a;
    yret b;
    return "presto\n";
}