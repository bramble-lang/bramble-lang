fn my_main( p: bool) {
    let c: co i64 := init my_co(5);
    let w1 : i64 := yield c;
    printiln w1;
    let w2 : i64 := yield c;
    printiln w2;
    printiln (yield c);
    printiln (yield c);
    printiln (yield c);
    printiln (yield c);
    printiln (yield c);
    return;
}

co my_co (x: i64) -> i64 {
    let mut i: i64 := x;
    return {
        mut i := i + 1;
        i - 1
    };
}