extern fn printf(fmt: string, ...);

fn write(s: string) {
    printf(s);
    return;
}

fn writei64(i: i64) {
    printf("%ld", i);
    return;
}

fn writei64ln(i: i64) {
    printf("%ld\n", i);
    return;
}

fn writei32(i: i32) {
    printf("%d", i);
    return;
}

fn writei32ln(i: i32) {
    printf("%d\n", i);
    return;
}